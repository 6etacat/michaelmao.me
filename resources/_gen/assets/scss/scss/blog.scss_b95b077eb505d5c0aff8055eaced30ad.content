.highlight{width:80%;margin:1.2em 0}.highlight pre{padding:1em 2em;border-radius:1ex}.highlight code{background-color:none;border-radius:0;padding:0}code{font-family:Hack,monospace;font-size:smaller;background-color:#2e3440;border-radius:.5ex;padding:.1ex .5ex}.taxonomy-info{margin-bottom:1.2em}.tags{display:flex;gap:1ex;margin-bottom:.2em}.series{color:#98a0a8}p{margin-bottom:1.2em}hr{margin:1.2em auto;background-color:#dde1e4;border-color:#dde1e4;color:#dde1e4;border-radius:2px;width:70%}h2{margin-top:1.2em;margin-bottom:.3em}h3{margin-top:1.2em;margin-bottom:.3em}h4{margin-top:1.2em;margin-bottom:.3em}h5{color:#98a0a8;font-size:1em;margin-top:1.2em;margin-bottom:.3em}h6{color:#98a0a8;font-size:.9em;margin-top:1.2em;margin-bottom:.3em}blockquote{width:80%;color:#98a0a8;margin-left:2em;margin-bottom:1.2em}blockquote blockquote{margin-bottom:.4em;margin-left:1em}blockquote p{margin-bottom:.4em}blockquote cite{display:block;text-align:right;font-style:italic}ul,ol{margin-left:1em;margin-bottom:1.2em;list-style-position:inside}.checklist{list-style-type:none}.footnotes li{margin-bottom:1.2em}.footnotes p{display:inline}