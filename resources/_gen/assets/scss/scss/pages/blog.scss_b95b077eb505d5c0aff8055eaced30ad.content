.highlight{width:80%;margin:1.2em 0;line-height:1.5}.highlight pre{padding:1em 2em;border-radius:1ex;overflow:auto}.highlight code{border-radius:0;padding:0;font-size:.8em;border-radius:.5ex}code{font-family:Hack,monospace;background-color:#2e3440;font-size:.8em;border-radius:.5ex;padding:.2ex .5ex}@media(orientation:portrait){.highlight{width:100%}}.taxonomy-info{margin-bottom:1.2em}.tags{display:flex;gap:1ex;font-size:1.1em;margin-bottom:.2em}.series{color:#98a0a8}hr{margin:1.2em auto;background-color:#dde1e4;border-color:#dde1e4;color:#dde1e4;border-radius:2px;width:70%}p{font-size:1em;margin-bottom:1.1em}h2{font-size:1.5em;margin-top:1.2em;margin-bottom:.3em}h3{font-size:1.3em;margin-top:1.2em;margin-bottom:.3em}h4{font-size:1.2em;margin-top:1.2em;margin-bottom:.3em}h5{color:#98a0a8;font-size:1.1em;margin-top:1.2em;margin-bottom:.3em}h6{color:#98a0a8;font-size:1em;margin-top:1.2em;margin-bottom:.3em}blockquote{width:80%;color:#98a0a8;margin-left:2em;margin-bottom:1.2em}blockquote blockquote{margin-bottom:.4em;margin-left:1em}blockquote p{margin-bottom:.4em}blockquote cite{display:block;text-align:right;font-style:italic}table{margin:1.2em;min-width:calc((100% - 2.4em) * .5);border-collapse:collapse;overflow:auto}table th,table td{border:1px solid #dde1e4;padding:1ex}ul,ol{margin-left:1em;margin-bottom:1.2em;list-style-position:inside}ul li,ol li{margin-bottom:.2em}.checklist{list-style-type:none}.footnotes li{margin-bottom:1.2em}.footnotes p{display:inline}.footnote-ref,.footnote-backref{user-select:none}@media(orientation:portrait){table{margin:1.2em auto}}